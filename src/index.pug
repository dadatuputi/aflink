- var USAFIcon = 'data:image/svg+xml;base64,'
- var GHIcon = 'data:image/svg+xml;base64,'
doctype html
html.bg-spice-brown(lang='en')
  head
    meta(charset='utf-8')
    meta(name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no')
    meta(name='description' content='')
    meta(name='author' content='')
    title USAF Links
    link(rel='icon' href=USAFIcon)
    //- CSS
    link(rel="stylesheet", href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/css/bootstrap.min.css", integrity="sha384-B0vP5xmATw1+K9KRQjQERJvTumQW0nPEzvF6L/Z6nronJ3oUOFUFpCjEUQouq2+l", crossorigin="anonymous")
    style
      include:clean-css site.css
    //- OpenSearch Description Document
    //- https://www.chromium.org/tab-to-search
    //- https://developer.mozilla.org/en-US/docs/Web/OpenSearch
    link(href="/osdd.xml" type="application/opensearchdescription+xml" title="aflink" rel="search")

  //- Navigation
  nav.navbar.navbar-expand-lg.navbar-dark.bg-spice-brown.sticky-top
    .container
      a.navbar-brand(href='#')
        img.d-inline-block.align-text-top(height='30' src=USAFIcon)
        |           USAF Links
      button.navbar-toggler(type='button' data-toggle='collapse' data-target='#navbarResponsive' aria-controls='navbarResponsive' aria-expanded='false' aria-label='Toggle navigation')
        span.navbar-toggler-icon
      #navbarResponsive.collapse.navbar-collapse
        .navbar-nav.ml-auto
          a.nav-link(aria-current='page' href='#') Home
          a.nav-link(href='#about') About
          a.nav-link(href='#correction') Add Link
  //- Page Content
  section#search.bg-sage.text-center
    .container
      .form-outline
        input#search-form.form-control(type='search' placeholder='Start typing' aria-label='Search' autofocus='')
  section#list.bg-sage.text-center
    .container
      h2 Link List
      div#link-list.list-group
        //- Links json is expanded here
        each category in links
          div.list-group
            //- Put a category here
            div.list-group-item.list-group-item-action.border-top.disabled.category #{category.name}
            //- Put each link in the category here
            each link in category.links
              a.list-group-item.list-group-item-action.border-top(href=link.link title=link.title) #{link.title}
      p.alert-dark(hidden) No results for 
        em
  section#about.bg-olive-drab.text-center
    .container
      h2 About
      p.lead
        | These links are mirrored from the USAF Quick Links page to be 
        | quickly and easily accessible from anywhere
  section#correction.bg-olive-drab.text-center
    .container
      h2 Request Link Addition
      p.lead
        | If you want to add a link, submit one through GitHub: 
        a(href='https://github.com/dadatuputi/aflinks/blob/master/README.md')
          img(width='25' src=GHIcon)
  section#footer.bg-spice-brown.py-1.text-right 
    em
      | Links synced #{date}  
  //- JavaScript
  script(src="https://code.jquery.com/jquery-3.5.1.slim.min.js"
    integrity="sha256-4+XzXVhsDmqanXGHaHvgh1gMQKX40OUvDEBTu8JcmNs="
    crossorigin="anonymous")
  script(src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/js/bootstrap.min.js", integrity="sha384-+YQ4JLhjyBLPDQt//I+STsc9iw4uQqACwlvpslubQzn4u2UU2UFM80nGisd026JF", crossorigin="anonymous")
  script 
    include:uglify-es search.js
